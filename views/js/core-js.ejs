<script>$('#iconNavbarSidenav').on('click', function(e) {
    if(!document.body.classList.contains('g-sidenav-pinned')){
        $('body').addClass('g-sidenav-pinned');
        e.stopPropagation();
    }else{
        $('body').removeClass('g-sidenav-pinned');
    }
});</script>